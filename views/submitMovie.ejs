
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>MovieShack</title>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="css/style.css" type="text/css" media="all" />
    <link class="cssdeck">
    <link rel="stylesheet" href="css/bootstrap.min.css" class="cssdeck">
    <link rel="stylesheet" href="css/bootstrap.css" class="cssdeck">
    <link rel="stylesheet" href="css/bootstrap-theme.min.css" class="cssdeck">

    <!--[if IE 6]><link rel="stylesheet" href="css/ie6-style.css" type="text/css" media="all" /><![endif]-->
    <!-- <script src="js/fns.js" type="text/javascript"></script> -->
    <!-- <script src="js/jquery-1.3.2.min.js" type="text/javascript"></script> -->
    <script src="js/jquery-1.3.2.min.js" type="text/javascript"></script>
    <script src="js/fns.js" type="text/javascript"></script>
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="js/searchmovie.js" type="text/javascript"></script>
    
</head>


<body>
<!-- Page -->

<div id="page" class="shell">
  <!-- Header -->
  <div id="header">
    <!-- Top Navigation -->
    <% include logOutMenuAdmin.ejs %>
    <!-- / Top Navigation -->
    <div class="cl">&nbsp;</div>
    <!-- Logo -->
    <div id="logo">
      <h1><a href="link to our home page/contact page">Movie<span>Shack</span></a></h1>
      <p class="description">Your Movie zone</p>
    </div><!-- / Logo -->
    
    <!-- Main Navigation --><!-- / Main Navigation -->
 <div class="cl">&nbsp;</div>
    <!-- Sort Navigation -->
  <div id="sort-nav">
    <div class="bg-right"> 

      <div class="bg-left">

        <% include Adminmenu.ejs %>


         
          <div class="container-fluid">
            <div class="row-fluid">
            
                <table class="table table-hover">
                <tr class="info">
                <td class="info">Movie Name</td>
                <td class="info">Categories</td>
                <td class="info">banner</td>
                <td class="info">rent</td>
                <td class="info">Copies</td>
                <td class="info">Customer's who has rented this</td>
                </tr class="info">
                
                    <% for (var i=0; i < rows.length ; i++) { %>
                     <tr class="info">
                     <td class="info"><%= rows[i].movie_name %></td>
                     <td class="info"><%= rows[i].categories %></td>
                     <td class="info"><%= rows[i].banner %></td>
                     <td class="info">$<%= rows[i].rent %></td>
                     <td class="info"><%= rows[i].copies %></td>
                     <% if ((rows[i].persons != null) && (typeof rows[i].persons != undefined) ) {%>
                          <td class="info"><%= rows[i].persons  %></td>
                     <% }else{ %>
                          <td class="info">No Customer has rented this yet</td>
                     <% } %> 
                   

                     
                     
                     </tr> 
                     <tr class="info">
                       
                        <% var link2 = "/searchcustomers_fromSubmit?"+rows[0].movie_id %>
                      <form action = <%=link2%>  method = "post"> 
                        <td><input type = "text" name = "customerInfo" id="customerInfo" placeholder = "Search Customer"></td>
                      <td><input class="btn btn-primary" id = "finduser" name = "finduser" type = "submit" value = "search"/></td>
                      </form>
                      <td class="info">&nbsp;&nbsp;</td>
                      <td class="info">&nbsp;&nbsp;</td>
                      <td class="info">&nbsp;&nbsp;</td>
                      <td class="info">&nbsp;&nbsp;</td>
                     
                      
                      </tr>
                          
                      <tr class="info">
                      <% var link3 = "/submitFromCustomer?"+rows[0].movie_id %>
                      <form action = <%=link3 %>  method = "post"> 
                      <td><input class="btn btn-primary" id = "submitButton" name = "submitButton" type = "submit" value="Return This Movie"/></td>
                      </form>
                      <td class="info">&nbsp;&nbsp;</td>
                      <td class="info">&nbsp;&nbsp;</td>
                      <td class="info">&nbsp;&nbsp;</td>
                      <td class="info">&nbsp;&nbsp;</td>
                      <td class="info">&nbsp;&nbsp;</td>
                     
                     </tr>
                    <% } %>
                </table>
           </div>
        </div>


        <table class="table table-hover">
       
          
    </script>
        </table>
</tr>

        </div>
      </div>
    </div>
    </div>
    <!-- / Sort Navigation -->
    
  </div>


  
  </div>
</div>

  </body>