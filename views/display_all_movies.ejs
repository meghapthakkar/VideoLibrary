

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>MovieShack</title>
<meta http-equiv="Content-type" content="text/html; charset=utf-8" />
<link rel="stylesheet" href="css/style.css" type="text/css" media="all" />
<link class="cssdeck">
<link rel="stylesheet" href="css/bootstrap.css" class="cssdeck">


<!--[if IE 6]><link rel="stylesheet" href="css/ie6-style.css" type="text/css" media="all" /><![endif]-->
<script src="js/jquery-1.3.2.min.js" type="text/javascript"></script>
<script src="js/searchmovie.js" type="text/javascript"></script>


</head>


<body>
<!-- Page -->

<div id="page" class="shell">
  <!-- Header -->
  <div id="header">
    <!-- Top Navigation -->
    <% include logOutMenuAdmin.ejs %>
    <!-- / Top Navigation -->
    <div class="cl">&nbsp;</div>
    <!-- Logo -->
    <div id="logo">
      <h1><a href="link to our home page/contact page">Movie<span>Shack</span></a></h1>
      <p class="description">Your Movie zone</p>
    </div><!-- / Logo -->
    
    <!-- Main Navigation --><!-- / Main Navigation -->
 <div class="cl">&nbsp;</div>
    <!-- Sort Navigation -->
  <div id="sort-nav">
    <div class="bg-right"> 

      <div class="bg-left">

        <% include Adminmenu.ejs %>


         
          <div class="container-fluid">
            <div class="row-fluid">
              <div id="msginfo"></div>
                <table class="table table-hover">
                  <tr class="info">
                
                  <td class="info">
                    <form method="post" action="/search_admin_movie">
                    <table>
                      <tr>
                        <td>
                          <input type="text" id="tbsearchmovie" name="tbsearchmovie" class="form-control" style="width:86%;" placeholder="Search Movie" tabindex="1">
                        </td>
                        <td>
                          <input type="submit" id="btnserachmovie" name="btnserachmovie" value="Search" class="btn btn-primary" tabindex="2">
                        </td>
                      </tr>
                    </table>
                  </form>
                  </td>
                <td class="info"></td>
               
                </tr >
                <tr class="info">
                <% if(rows != 'undefined' && rows != null && rows != ""){%>
                      <td class="info">Movie Name</td>
                    <td class="info">Banner</td>
                <%}else{%>
                      <td class="info" colspan="2"><b>No such movie found</b></td>
                <%}%>
                
               
                </tr >
                <% if(rows != 'undefined' && rows != null && rows != ""){%>
                
                      <% for (var i=0; i < rows.length ; i++) { %>
                       <tr class="info">
                        <% var link1 = "/admin_individual_movies?"+rows[i].movie_id %>
                       
                       <td class="info"><a href=<%=link1 %>><%= rows[i].movie_name %></a></td>
                       <td class="info"><%= rows[i].banner %></td>
                       
                       </tr> 
                  <% } %>
              

                 <% } %>

        </table>
           </div>
        </div>


        <table class="table table-hover">
  ...
</table>
</tr>

        </div>
      </div>
    </div>
    </div>
    <!-- / Sort Navigation -->
    
  </div>


  
  </div>
</div>
  </body>